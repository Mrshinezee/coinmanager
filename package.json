{
  "name": "coinmanager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "0.62.3",
    "react-native-calendars": "^1.203.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-responsive-screen": "^1.2.2",
    "react-native-table-component": "^1.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "^24.8.0",
    "detox": "^13.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-dom": "^16.8.6",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "16.8.3",
    "redux-logger": "^3.0.6",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-rn-debugger": "^0.8.4"
  },
  "remotedev": {
    "hostname": "localhost",
    "port": 5678
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
    "setupFiles": [
      "./jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "./node_modules/(?!(react-native|react-native-vector-icons|redux-persist|react-native-responsive-screen|@react-native-community/async-storage)/)"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
       "ios.sim.debug": {
         "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/coinmanager.app",
         "build": "xcodebuild -workspace ios/coinmanager.xcworkspace -scheme coinmanager -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
         "type": "ios.simulator",
         "name": "iPhone 8"
       }
     }
  }
}
